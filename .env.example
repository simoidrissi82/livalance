# Base URL for the application (used in metadata and structured data)
NEXT_PUBLIC_BASE_URL=https://www.livalance.example

# Plausible Analytics domain (leave empty to disable analytics)
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=

# Node version for Cloudflare Pages
NODE_VERSION=20

# Disable Next.js telemetry (optional)
NEXT_TELEMETRY_DISABLED=1
