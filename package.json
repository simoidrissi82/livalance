{
  "name": "livalance",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "npx @cloudflare/next-on-pages build",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,md,mdx,json}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx,json}\"",
    "typecheck": "tsc --noEmit",
    "generate:sitemap": "cross-env SITEMAP_LOCALE=de next-sitemap && cross-env SITEMAP_LOCALE=en next-sitemap",
    "pages:build": "npx @cloudflare/next-on-pages build",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.11",
    "autoprefixer": "^10.4.17",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^12.23.24",
    "next": "^16.0.1",
    "next-intl": "^4.5.0",
    "next-mdx-remote": "^5.0.0",
    "next-sitemap": "^4.2.0",
    "postcss": "^8.4.35",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rehype-autolink-headings": "^7.0.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "swiper": "^12.0.3",
    "tailwindcss": "^3.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@types/aria-query": "^5.0.4",
    "@types/node": "^20.11.16",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "ansi-regex": "^5.0.1",
    "ansi-styles": "^5.2.0",
    "aria-query": "^5.3.0",
    "cross-env": "^7.0.3",
    "dom-accessibility-api": "^0.5.16",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "lz-string": "^1.5.0",
    "picomatch": "^4.0.3",
    "prettier": "^3.2.5",
    "pretty-format": "^27.5.1",
    "react-is": "^17.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^4.0.7",
    "wrangler": "^4.46.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
